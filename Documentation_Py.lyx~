\begin_layout Section
Usage
\end_layout

\begin_layout Standard
After installation, from within a GRASS-GIS session, see help details via
 i.fusion.hpf --help -- also provided here:
\end_layout

\begin_layout Standard
\begin_inset listings
lstparams "basicstyle={\small\ttfamily},breaklines=true,tabsize=4"
inline false
status open

\begin_layout Plain Layout

Description:
\end_layout

\begin_layout Plain Layout

 Fusing high resolution Panchromatic and low resolution Multi-Spectral data
 based on the High-Pass Filter Addition technique (Gangkofner, 2008)
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

Keywords:
\end_layout

\begin_layout Plain Layout

 imagery, fusion, HPF, HPFA
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

Usage:
\end_layout

\begin_layout Plain Layout

 i.fusion.hpf [-l2] pan=string msx=string[,string,...]
\end_layout

\begin_layout Plain Layout

   outputprefix=string [ratio=value] [center=string] [center2=string]
\end_layout

\begin_layout Plain Layout

   [modulation=string] [modulation2=string] [--help] [--verbose]
\end_layout

\begin_layout Plain Layout

   [--quiet]
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

Flags:
\end_layout

\begin_layout Plain Layout

  -l   Linearly match histogram of Pan-sharpened output to Multi-Spectral
 input
\end_layout

\begin_layout Plain Layout

  -2   2-Pass Processing (recommended) for large resolution ratio (>=5.5)
\end_layout

\begin_layout Plain Layout

 --h   Print usage summary
\end_layout

\begin_layout Plain Layout

 --v   Verbose module output
\end_layout

\begin_layout Plain Layout

 --q   Quiet module output
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

Parameters:
\end_layout

\begin_layout Plain Layout

           pan   High resolution panchromatic image
\end_layout

\begin_layout Plain Layout

           msx   Low resolution multi-spectral image(s)
\end_layout

\begin_layout Plain Layout

  outputprefix   Prefix for the Pan-Sharpened Multi-Spectral image(s)
\end_layout

\begin_layout Plain Layout

                 default: hpf
\end_layout

\begin_layout Plain Layout

         ratio   Custom ratio overriding standard calculation
\end_layout

\begin_layout Plain Layout

                 options: 1.0-10.0
\end_layout

\begin_layout Plain Layout

        center   Center cell value of the High-Pass-Filter
\end_layout

\begin_layout Plain Layout

                 options: low,mid,high
\end_layout

\begin_layout Plain Layout

                 default: low
\end_layout

\begin_layout Plain Layout

       center2   Center cell value for the second High-Pass-Filter (use
 -2 flag)
\end_layout

\begin_layout Plain Layout

                 options: low,mid,high
\end_layout

\begin_layout Plain Layout

                 default: low
\end_layout

\begin_layout Plain Layout

    modulation   Level of modulating factor weighting the HPF image to determine
 crispness
\end_layout

\begin_layout Plain Layout

                 options: min,mid,max
\end_layout

\begin_layout Plain Layout

                 default: mid
\end_layout

\begin_layout Plain Layout

   modulation2   Level of modulating factor weighting the second HPF image
 to determine crispness (use -2 flag)
\end_layout

\begin_layout Plain Layout

                 options: min,mid,max
\end_layout

\begin_layout Plain Layout

                 default: mid
\end_layout

\begin_layout Plain Layout

                  min: Minimum: 0.25
\end_layout

\begin_layout Plain Layout

                  mid: Mid: 0.35
\end_layout

\begin_layout Plain Layout

                  max: Maximum: 0.5
\end_layout

\end_inset


\end_layout

\begin_layout Subsection
Remarks
\end_layout

\begin_layout Itemize
requires (currently) manual color rebalancing (e.g.
 by using i.colors.enhance)
\end_layout

\begin_layout Itemize
easy to use, i.e.: for one band i.fusion.hpf pan=Panchromatic msx=${Band} for
 multiple bands i.fusion.hpf pan=Panchromatic msx=Red,Green,Blue,NIR
\end_layout

\begin_layout Itemize
easy to test various parameters that define the High-Pass filter’s kernel
 size and center value
\end_layout

\begin_layout Itemize
should work with any kind of imagery (think of bitness)
\end_layout

